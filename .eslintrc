{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "jasmine": true,
    "jest": true
  },
  "rules": {
    "func-names": "off",
    "prefer-arrow-callback": "off",
    "no-console": "off",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  }
}
